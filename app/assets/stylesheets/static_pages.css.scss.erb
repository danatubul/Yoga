

body {
    overflow: hidden;
}
.wrapper {
    # background-color:red;
    Position: absolute;
    top: 0; 
    left: 0; 
    bottom: 0; 
    right: 0;
    min-width: 965px;
}

.header {
    width: 100%;
    height: 41px;
    background-color:#00334d;
    display: flex;
    justify-content: space-between;

}

.leftUp{
    width: 88px;
    height: 51px;
    float: left;
    margin: -4px 15px 0px 25px;

    background-image: url(<%= asset_path'spectory_logo.png' %>);
    
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: center;
}

.rightUp{
    width: 92px;
    height: 27px;
    float: right;
    margin: 7px 18px 0px 0px;
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
    border: 1px solid white;
}

.rightUpL{
    color: white;
    font-size: 10px;
    width: 67px;
    height: 20px;
    margin: 0px 0px -2px 5px;
}

.rightUpR{
    width: 12px;
    height: 23px;
    color :white;
}

.backG {
    width: 100%;
    height: 615px;
    background-image: url(<%= asset_path'bg_small.jpg' %>);
    background-repeat:no-repeat;
    background-size: cover;
}

.grandPractice {
    width: 100%;
    height: 153px;
    display: flex;
    justify-content: center;
}

.headPic {
    width: 220px;
    height: 198px;
    background-image: url(<%= asset_path'Bootcamp_logo.png' %>);
    background-size: contain;
    background-repeat:no-repeat;
    background-position: center;
}
      
.carousel {
    width: 100%;
    height: 192px;
    display: flex;
    justify-content: center;
    align-items: center;
}

.carouselBox {
    width: 80%;
    height: 152px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: white;
    # opacity: 0.5;
}
.carouselCenter{
    width: 90%;
    height: 99px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin: -15px -0px 0px 0px;
}

@media screen and (max-width: 1190px) and (min-width: 960px) {

.carouselBox {
    width: 960px ;
    height: 152px;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.carouselCenter{
    width: 90%;
    height: 99px;
    display: flex;
    align-items: center;
    display: flex;
    justify-content: space-between;
    margin: -15px -0px 0px 0px;
}
}

@media screen and (orientation:portrait) {

.carouselBox {
    width: 960px;
    height: 152px;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.carouselCenter{
    width: 90%;
    height: 99px;
    display: flex;
    align-items: center;
    display: flex;
    justify-content: space-between;
    margin: -15px -0px 0px 0px;
}
}

.carouselR{
    width: 15px;
    height: 99px;
    background-image: url(<%= asset_path'forward_forward.png' %>);
    background-size: contain;
    background-repeat:no-repeat;
    background-position: center;
    margin: -30px 9px 0px 0px;
}

.carouselL{
    width: 15px;
    height: 99px;
    background-image: url(<%= asset_path'arrow_back.png' %>);
    background-size: contain;
    background-repeat:no-repeat;
    background-position: center;
    margin: -30px 0px 0px 9px;
}

.imgEmployee{
    font-size: 12px;
    display: flex;
    align-items: center;
    flex-direction: column;
}

.imgEmployeeBold{
    font-size: 12px;
    font-weight: bold;
    display: flex;
    align-items: center;
    flex-direction: column;
}
.nameLength{
    margin: 7px 0px 0px 0px;

}
.myImg {
    border-radius: 50%;
    width: 70px;
    height: 70px;
    filter: grayscale(100%);
}

.myImgRed {
    border-radius: 50%;
    width: 70px;
    height: 70px;
}

.borderNormal {
    border-radius: 50%;
