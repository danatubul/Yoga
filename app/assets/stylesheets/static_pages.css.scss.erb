    height: 70px;
    border-style: solid;
    border-color:#6699cc;
    justify-content: center;
}

.borderRed {
    border-radius: 50%;
    width: 70px;
    height: 70px;
    border-style: solid;
    border-color: #ff3333;
    justify-content: center;
}

.selectedImg {
    border-radius: 50%;
